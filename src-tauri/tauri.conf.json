{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "leaf",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../app/build",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "cd app &&npm run dev",
    "beforeBuildCommand": "cd app && npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "leaf",
        "width": 800,
        "height": 600,
        "minWidth": 620,
        "minHeight": 400,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
